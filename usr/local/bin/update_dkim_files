#!/bin/busybox sh

# Exit if an error occurs or an unset variable is referenced.
set -euo pipefail

# By default, don't let any non-privileged user access files created by this
# script.
umask 0077

# Copy the contents of the new DKIM private key to the DKIM database directory.
cat /home/dkim/keys/default.private > /var/db/dkim/default.private
